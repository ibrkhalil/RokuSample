            <Button id="Skip 15" iconUri="https://img.icons8.com/ios/50/000000/circled-right-2.png" focusedIconUri="https://img.icons8.com/ios-filled/50/000000/circled-right-2.png" minWidth="110" maxWidth="140" />
            <Button id="CC" iconUri="https://img.icons8.com/ios/50/000000/quote--v1.png" focusedIconUri="https://img.icons8.com/ios-filled/50/000000/quote.png" minWidth="110" maxWidth="140" />
        </LayoutGroup>
