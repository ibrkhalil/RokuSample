<?xml version="1.0" encoding="UTF-8"?>
<component name="trickPlay" extends="Group">
    <script type="text/brightscript" uri="TrickPlay.brs" />
    <children>
        <LayoutGroup horizAlignment="center" vertAlignment="top">
            <Label height="100" id="currentTime" />
            <Poster height="8" width="700" color="0x333333FF" />
            <Poster height="8" width="700" color="0xFFFFFFFF" />
